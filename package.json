{
	"name": "all-or-nothing-game",
	"version": "1.12.0",
	"type": "module",
	"browserslist": "chrome > 140",
	"scripts": {
		"dev": "vite",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit",
		"build": "vite build",
		"build:web": "vite build",
		"build:desktop": "npm run build:desktop:linux && npm run build:desktop:windows",
		"build:desktop:linux": "npm run build:desktop:prepare && node scripts/build-desktop.ts --platform=linux",
		"build:desktop:windows": "npm run build:desktop:prepare && node scripts/build-desktop.ts --platform=win",
		"build:desktop:prepare": "node scripts/prepare-desktop.ts",
		"package:appimage": "node scripts/package-appimage.ts",
		"package:linux": "npm run package:appimage",
		"package:all": "npm run package:linux",
		"release:linux": "npm run build && npm run build:desktop:linux && npm run package:linux",
		"release:windows": "npm run build && npm run build:desktop:windows",
		"release:desktop": "npm run build && npm run build:desktop && npm run package:all",
		"release:all": "npm run release:desktop"
	},
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@fontsource/roboto": "^5.2.10",
		"@mui/icons-material": "^7.3.8",
		"@mui/material": "^7.3.8",
		"@types/howler": "^2.2.12",
		"@types/lodash.debounce": "^4.0.9",
		"@types/node": "^25.3.2",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.4",
		"@vitejs/plugin-react-swc": "^4.2.3",
		"canvas-confetti": "^1.9.4",
		"dexie": "^4.3.0",
		"dexie-react-hooks": "^4.2.0",
		"framer-motion": "^12.34.3",
		"jotai": "^2.18.0",
		"lodash.debounce": "^4.0.8",
		"nw-builder": "^4.17.5",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-ga4": "^2.1.0",
		"typescript": "^5.9.3",
		"use-sound": "^5.0.0",
		"usehooks-ts": "^3.1.1",
		"vite": "^7.3.1",
		"vite-plugin-pwa": "^1.2.0",
		"vite-plugin-svgr": "^4.5.0"
	},
	"devDependencies": {
		"@types/canvas-confetti": "^1.9.0"
	}
}
